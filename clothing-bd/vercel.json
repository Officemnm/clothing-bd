{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/cron/snapshot?secret=${CRON_SECRET}&slot=slot1",
      "schedule": "45 6 * * *"
    },
    {
      "path": "/api/cron/snapshot?secret=${CRON_SECRET}&slot=slot2",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/cron/snapshot?secret=${CRON_SECRET}&slot=slot3",
      "schedule": "0 15 * * *"
    }
  ]
}
